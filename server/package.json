    "main": "index.js",
    "scripts": {
        "start": "node ./build/index.js",
        "dev": "nodemon --legacy-watch ./src/index.ts",
        "build": "npx tsc",
        "up": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up -d",
        "down": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml down",
        "dcbuild": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml build",
        "dcbuildp": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml build",
        "dcpushp": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml push",
        "dcpullp": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml pull"
    },
